{
  "name": "vue-webpack",
  "version": "1.0.0",
  "description": "vue2.x+webpack4.x工程",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js",
